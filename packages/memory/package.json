{
  "name": "@astrale/typegraph-adapter-memory",
  "version": "0.2.1",
  "description": "In-memory adapter for typegraph - zero infrastructure graph database",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "graph",
    "database",
    "in-memory",
    "typegraph",
    "typescript",
    "type-safe"
  ],
  "author": "Astrale",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astrale-os/typegraph.git",
    "directory": "packages/memory"
  },
  "dependencies": {
    "@astrale/typegraph": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "5.8.3",
    "vitest": "^2.1.8",
    "zod": "^3.24.1"
  },
  "engines": {
    "node": ">=22"
  }
}
